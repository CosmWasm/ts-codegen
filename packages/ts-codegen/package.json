{
  "name": "@cosmwasm/ts-codegen",
  "version": "1.8.0",
  "description": "@cosmwasm/ts-codegen converts your CosmWasm smart contracts into dev-friendly TypeScript classes so you can focus on shipping code.",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/cosmwasm/ts-codegen",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "bin": {
    "cosmwasm-ts-codegen": "ts-codegen.js",
    "ts-codegen": "ts-codegen.js"
  },
  "scripts": {
    "copy": "copyfiles -f LICENSE-Apache LICENSE-MIT README.md package.json dist",
    "clean": "del dist/**",
    "prepare": "npm run build",
    "cmds": "ts-node ./scripts/cmds.ts",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "ts-node src/ts-codegen",
    "file": "ts-node src/file"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmwasm/ts-codegen"
  },
  "bugs": {
    "url": "https://github.com/cosmwasm/ts-codegen/issues"
  },
  "devDependencies": {
    "@cosmjs/cosmwasm-stargate": "0.30.1",
    "ast-stringify": "0.1.0"
  },
  "dependencies": {
    "@babel/generator": "7.24.4",
    "@babel/traverse": "7.24.1",
    "@babel/types": "7.24.0",
    "@cosmwasm/ts-codegen-types": "^1.2.0",
    "@pyramation/json-schema-to-typescript": " 11.0.4",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.15",
    "case": "1.6.3",
    "dargs": "7.0.0",
    "deepmerge": "4.2.2",
    "fuzzy": "0.1.3",
    "glob": "8.0.3",
    "inquirerer": "0.1.3",
    "minimist": "1.2.6",
    "mkdirp": "1.0.4",
    "nested-obj": "0.0.1",
    "parse-package-name": "1.0.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.5",
    "wasm-ast-types": "^1.5.0"
  },
  "keywords": [
    "cosmwasm",
    "interchain",
    "smart contracts",
    "codegen"
  ]
}
